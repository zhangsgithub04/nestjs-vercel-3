{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },

"modules": [
    {
      "type": "app",
      "class": "AppModule",
      "path": "./app.module"
    },
    {
      "type": "module",
      "class": "instructor",
      "path": "./instructor/instructor.module"
    },
    {
      "type": "module",
      "class": "question",
      "path": "./question/question.module"
    }
  ]


}
